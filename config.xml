<?xml version='1.0' encoding='utf-8'?>
<widget id="com.anytech.guianarrativas" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GUIA OS OBJETOS E SUAS NARRATIVAS</name>
    <description>
        ...
    </description>
    <author email="contato@anytech.com" href="https://www.facebook.com/anytechOficial/">
        ANYTECH
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
		
        <allow-intent href="market:*" />
		
		<icon src="www/logo.ico" density="ldpi" />
		<icon src="www/logo.ico" density="mdpi" />
		<icon src="www/logo.ico" density="hdpi" />
		<icon src="www/logo.ico" density="xhdpi" />
		<icon src="www/logo.ico" density="xxhdpi" />
		<icon src="www/logo.ico" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    
    <preference name="android-minSdkVersion" value="22" />
	<preference name="android-targetSdkVersion" value="22" />
</widget>
